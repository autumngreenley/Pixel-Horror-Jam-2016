            <string>draw_set_font(f_chat);
draw_set_color(c_black);

message_end= array_length_1d(message);


if(message_end&gt;0){
    tX=10;
    tY=view_hview[0] - 105;
    
    
    var charWidth = 15.5;
    var lineEnd = 40;
    var line = 0;
    var space = 0;
    var i = 1;
    
    var delay = 1;
    
    
    if(cutoff &lt; string_length(message[message_current])){
        if(timer &gt;= delay){
            cutoff++;
            timer = 0;
        }
        else{
            timer++;
        }
    
    }
    
    
    if(keyboard_check_pressed(ord('S'))){
        
        if(cutoff &lt; string_length(message[message_current])){
            cutoff=string_length(message[message_current]);
        }
        else if(message_current &lt; message_end-1){
            message_current++;
            cutoff=0;
            //total_width=0;
        }
        else{
            done=true;
        }
        if(done==true){
            room_goto_next();
        }
    }
    
    
    while(i &lt;= string_length(message[message_current]) &amp;&amp; i &lt;= cutoff){
        var length=0;
        //var total_width=0;
        while(string_char_at(message[message_current], i) != " " &amp;&amp; i &lt;= string_length(message[message_current])){
            i++;
            length++;
        //    total_width+=test3;
        }
        if(space + length &gt; lineEnd){
            space = 0;
            line++;
        }
        
        i-=length;
        
       // test2 = string_char_at(message[message_current], i);
       // test3 = string_width(test2);
       // charWidth=test3;
       
        draw_text(tX+(space*charWidth), tY+(32*line), string_char_at(message[message_current], i));
        //draw_text(tX+total_width, tY+(13*line), string_char_at(message[message_current], i));

        space++;
        i++;
    }
}

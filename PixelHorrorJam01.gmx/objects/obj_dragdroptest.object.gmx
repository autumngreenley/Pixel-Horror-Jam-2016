        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Moving the body part with the mouse

//If the bodyshelf is currently closed then you can move the body part.
//If the shelf is open, but the body part is the the right of the shelf, you can move it.
//If the body part is on the shelf, you can move it.
if( obj_BodyShelf.currently_closed==true    ||      (obj_BodyShelf.currently_closed==false &amp;&amp; x&gt;400)    ||  onshelf==true){
    canmove=true;
}
//You cannot move the body part if it is beneath the shelf.
else{
    canmove=false;
}

//If you have just begun to drag the body part, then it sets where you're holding it from
if(initial==true){
    mousedifferencex=mouse_x-x;
    mousedifferencey=mouse_y-y;
}

//If you're dragging the body part, move it to where your mouse is
if(drag==true){
    x=mouse_x - mousedifferencex;
    y=mouse_y - mousedifferencey;
    initial=false;
}
</string>
          </argument>
        </arguments>
      </action>
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Checking the body part with the shelf

//(If the shelf is open and you try to close it with the body part on it...)
if( obj_BodyShelf.currently_closed == false &amp;&amp; 
    x &lt; 400 &amp;&amp; 
    mouse_check_button_released(mb_left))&amp;&amp;
    active==true{
    
    currentx=x;
    onshelf=true;
}

//(If the body part is being held or is on the shelf, then it is considered active)
if(onshelf==true){
    active=true;
}
else if(drag==true){
    active=true;
}
else{
    active=false;
}

//(If the body part is on the shelf and the shelf is closed...)
if(onshelf==true &amp;&amp; obj_BodyShelf.currently_closed==true){
    x=-300;
}

//(If the body part is on the shelf and the shelf is open...)
if(onshelf==true &amp;&amp; obj_BodyShelf.currently_closed==false){
    x=currentx;
}

//(If the body part is on an open shelf, and the user tries to move it off...)
if(onshelf==true &amp;&amp; drag==true &amp;&amp; obj_BodyShelf.currently_closed==false){
    onshelf=false;
}

//(If the body part is active, then it is above the shelf)
if(active==true){
    depth=-500;
}
//(If the body part is inactive, then it is below the shelf)
else{
    depth=0;
}
</string>
          </argument>
        </arguments>
      </action>
